{
  "name": "react-star-rating",
  "description": "A simple star rating component built with React.",
  "main": "./dist/react-star-rating.min.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "webpack-dev-server --content-base example --inline --hot",
    "build": "gulp dist",
    "lint": "./node_modules/.bin/eslint ./src",
    "check-coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover _mocha -- -R spec -r test/setup.js --recursive test --compilers js:babel-core/register",
    "test:watch": "mocha -r setup.js -w --compilers js:babel-core/register",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Cameron Roe <cameronjroe@gmail.com> (http://cameronjroe.com/)",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.0",
    "jss": "^3.2.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-jss": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.2",
    "chai": "^3.4.0",
    "chai-enzyme": "0.4.2",
    "chai-equal-jsx": "^1.0.9",
    "codecov.io": "0.1.6",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "jsdom": "8.5.0",
    "jshint-stylish": "^2.0.1",
    "json-loader": "0.5.4",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.1",
    "react-addons-test-utils": "^15.0.1",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.2",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  }
}
